### Sheet
. $APPLDIR/sheets/00.sheet

usedb=${usedb:-$( Ask 'Do you want to use DB ?' )}
if [[ $usedb = 'y' ]];then
  lsnr=e001
  oraver=11g
  orahom=/u43/e001/oracle/db/tech_st/11.2.0
  oradat=/u43/e001/oracle/db/apps_st/data/e001
  alertlog=/u43/e001/oracle/diag/rdbms/e001/e001/trace/alert_e001.log
  cat >> ${stxt} <<EOS
$ . $orasid.env

$ echo \$ORACLE_SID
