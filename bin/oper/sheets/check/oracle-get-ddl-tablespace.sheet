#!/bin/sh -u

### Local Parameter
sid=${sid:-$( Input 'DB SID' )}
oty=${oty:-$( Input 'Object Type' )}
onm=${onm:-$( Input 'Object Name' )}

### Sheet
cat >> ${stxt} <<EOF

