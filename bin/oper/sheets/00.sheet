#!/bin/sh -u

localip=10.213.32.246

### Sheet
cat >> ${stxt} <<EOF


######################################################################
# 00. ${host} に ${user} でログイン
######################################################################
$ whoami ; id ; uname -n 
	${user}
	{*}(${user}) {*}
	${host}
	#! ホスト名、ユーザ名を確認
	
$ pwd
	${homedr}

$ echo \${\$}

$ date

$ alias lftp='ftp -niv ${localip}'
	#! Local PC へのファイル転送用

$ export PS1='$ '
	#! プロンプト'$ 'は'cmd.log'に出力される


EOF
